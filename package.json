{
 "name": "threejs-es6-webpack-boilerplate",
 "version": "1.0.0",
 "description": "Boilerplate for Three.js projects set up with Babel for ES6 and compiled with webpack",
 "author": "-",
 "main": "app.js",
 "scripts": {
  "dev": "webpack-dev-server --open --config ./config/webpack.dev.conf.js",
  "build": "webpack --config ./config/webpack.build.conf.js",
  "lint": "eslint --debug src/",
  "lint:write": "eslint --debug src/ --fix",
  "prettier": "prettier --write src/**/*.js"
 },
 "browserslist": [
  "> 1%",
  "last 3 version"
 ],
 "husky": {
  "hooks": {
   "pre-commit": "lint-staged"
  }
 },
 "lint-staged": {
  "*.(js|jsx)": [
   "npm run lint:write",
   "npm run prettier",
   "git add"
  ]
 },
 "dependencies": {
  "es6-promise": "^4.2.5",
  "three": "^0.100.0",
  "three-obj-loader": "^1.1.3",
  "tween.js": "^16.6.0"
 },
 "devDependencies": {
  "@babel/core": "^7.2.2",
  "@babel/preset-env": "^7.3.1",
  "autoprefixer": "^9.4.7",
  "babel-eslint": "^10.0.3",
  "babel-loader": "^8.0.5",
  "babel-plugin-module-resolver": "^3.2.0",
  "babel-polyfill": "^6.26.0",
  "copy-webpack-plugin": "^5.0.4",
  "copyfiles": "^2.1.0",
  "css-loader": "^2.1.0",
  "cssnano": "^4.1.10",
  "eslint": "^5.12.1",
  "eslint-config-prettier": "^6.5.0",
  "eslint-import-resolver-babel-module": "^5.1.0",
  "eslint-plugin-babel": "^5.3.0",
  "eslint-plugin-import": "^2.18.2",
  "eslint-plugin-jest": "^23.0.2",
  "eslint-plugin-prettier": "^3.1.1",
  "html-webpack-plugin": "^3.2.0",
  "husky": "^1.1.2",
  "lint-staged": "^7.3.0",
  "mini-css-extract-plugin": "^0.5.0",
  "node-sass": "^4.11.0",
  "npm-run-all": "^4.1.5",
  "optimize-css-assets-webpack-plugin": "^5.0.3",
  "postcss-loader": "^3.0.0",
  "prettier": "^1.18.2",
  "rimraf": "^2.6.3",
  "sass-loader": "^7.1.0",
  "style-loader": "^0.23.1",
  "uglifyjs-webpack-plugin": "^2.1.1",
  "webpack": "^4.29.0",
  "webpack-bundle-analyzer": "^3.6.0",
  "webpack-cli": "^3.2.1",
  "webpack-copy-plugin": "0.0.4",
  "webpack-dev-server": "^3.1.14",
  "webpack-merge": "^4.2.2"
 }
}
